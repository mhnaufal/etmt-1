{
  "name": "etmt-1",
  "version": "0.0.1",
  "description": "Express Typescript TypeORM MySql Mail Management System",
  "main": "index.js",
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "start:dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "start:prod": "npm run build && NODE_ENV=production node dist/index.js",
    "build": "tsc -p tsconfig.prod.json && npx tsc-alias",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- -n",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:reset": "npx rimraf dist/src/db && npx rimraf src/db",
    "heroku-postbuild": "npm run migration:reset && npm run migration:generate first && npm run migration:run",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -n ormconfig.ts",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhnaufal/etmt-1.git"
  },
  "keywords": [
    "express",
    "typescript",
    "typeorm",
    "mysql"
  ],
  "author": "Muhammad Naufal Pratama",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mhnaufal/etmt-1/issues"
  },
  "engines": {
    "node": "16.13"
  },
  "homepage": "https://github.com/mhnaufal/etmt-1#readme",
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "connect-flash": "^0.1.1",
    "connect-typeorm": "^1.1.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-handlebars": "^6.0.2",
    "express-session": "^1.17.2",
    "method-override": "^3.0.0",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1",
    "typeorm-static": "^1.1.1",
    "winston": "^3.5.0",
    "winston-daily-rotate-file": "^4.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/connect-flash": "^0.0.37",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^6.0.0",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.0",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.20",
    "@types/node": "^17.0.12",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
